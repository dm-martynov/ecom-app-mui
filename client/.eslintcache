tch, useSelector } from 'react-redux'\nimport { signInStart } from '../redux/auth/auth.actions'\n\nconst useStyles = makeStyles((theme) => ({\n  paper: {\n    marginTop: theme.spacing(20),\n    display: 'flex',\n    flexDirection: 'column',\n    alignItems: 'center',\n  },\n\n  form: {\n    width: '100%', // Fix IE 11 issue.\n    marginTop: theme.spacing(3),\n  },\n  submit: {\n    margin: theme.spacing(2, 0, 2),\n    paddingTop: 12,\n    paddingBottom: 12,\n  },\n  buttonProgress: {\n    color: 'green[500]',\n    position: 'absolute',\n    top: '50%',\n    left: '50%',\n    marginTop: -12,\n    marginLeft: -12,\n  },\n}))\n\nexport default function SignIn() {\n  const classes = useStyles()\n  const dispatch = useDispatch()\n  const loading = useSelector((state) => state.auth.loading)\n\n  return (\n    <Formik\n      initialValues={{\n        email: '',\n        password: '',\n      }}\n      validate={(values) => {\n        const errors = {}\n        if (!values.email) {\n          errors.email = 'Required'\n        }\n        if (!values.password) {\n          errors.password = 'Required'\n        }\n        return errors\n      }}\n      onSubmit={(values) => {\n        dispatch(signInStart(values))\n      }}\n    >\n      {({ submitForm, isSubmitting, touched, errors }) => (\n        <Container component='main' maxWidth='xs'>\n          <CssBaseline />\n          <div className={classes.paper}>\n            <Typography component='h1' variant='h5'>\n              Sign in\n            </Typography>\n            <Form className={classes.form}>\n              <Grid container spacing={2}>\n                <Grid item xs={12}>\n                  <Field\n                    component={TextField}\n                    type='email'\n                    required\n                    label='Email Address'\n                    name='email'\n                    variant='outlined'\n                    fullWidth\n                  />\n                </Grid>\n                <Grid item xs={12}>\n                  <Field\n                    component={TextField}\n                    label='Password'\n                    name='password'\n                    variant='outlined'\n                    type='password'\n                    id='password'\n                    required\n                    fullWidth\n                  />\n                </Grid>\n              </Grid>\n              <FormControlLabel\n                control={<Checkbox value='remember' color='primary' />}\n                label='Remember me'\n              />\n\n              <div style={{ position: 'relative' }}>\n                <Button\n                  fullWidth\n                  variant='contained'\n                  color='primary'\n                  disabled={loading}\n                  className={classes.submit}\n                  onClick={submitForm}\n                >\n                  Sign In\n                </Button>\n                {loading && (\n                  <CircularProgress\n                    size={24}\n                    className={classes.buttonProgress}\n                  />\n                )}\n              </div>\n\n              <Grid container>\n                <Grid item xs>\n                  <Link href='#' variant='body2'>\n                    Forgot password?\n                  </Link>\n                </Grid>\n                <Grid item>\n                  <Link component={RouterLink} to='/sign-up' variant='body2'>\n                    Don't have an account? Sign Up\n                  </Link>\n                </Grid>\n              </Grid>\n            </Form>\n          </div>\n          <Box mt={4}>\n            <Typography variant='body2' color='textSecondary' align='center'>\n              {`Copyright Â©  Pet Project ${new Date().getFullYear()}`}\n            </Typography>\n          </Box>\n        </Container>\n      )}\n    </Formik>\n  )\n}\n","/home/dmitrii/github_repos/ecom-app-mui/client/src/reportWebVitals.js",[]